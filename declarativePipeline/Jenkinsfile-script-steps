pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                script{
                    def name = 'Alex'
                    if (name == 'Alex')
                        prinln "Hi $name"
                    else
                        println 'Hi anonymous'
                    sleep 2
                    echo 'Finalizando a Build'
                }
            }
        }
    }
}
