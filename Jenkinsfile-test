def name1 = 'Alexandre'
def name2 = 'Basilio'

pipeline {
    agent any
    stages {
        stage('Salutation World') {
            environment {
                name3 = 'Jenifer'
            }
            steps {
                echo "Name1 ${name1}"
                echo "Name2 ${name2}"
                echo "Name3 ${name3}"
            }
        }

    }
}