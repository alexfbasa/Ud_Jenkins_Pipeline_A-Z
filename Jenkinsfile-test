def name1 = ''
def name2 = ''

pipeline {
    agent any
    stages {
        stage('Salutation World') {
            environment {
                name3 = 'Jenifer'
            }
            steps {
                echo "Name1 ${name1}"
                echo "Name2 ${name2}"
                echo "Name3 ${name3}"
                sh "printenv"
            }
        }
        stage('Check varibles'){
            steps {
                sh "printenv"
            }
        }

    }
}