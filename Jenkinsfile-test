job('NodeJs example') {
    scm {
        git('git://github.com/alexbasa/docker-demo.git') { node ->
            node / gitConfigName('DSL User')
            node / gitConfigEmail('alexsimple@gmail.com')
        }
    }
    triggers {
        scm('H/5 * * * *')
    }
    wrappers {
        nodejs('nodejs')
    }
    steps {
        shell('npm install')
    }

}