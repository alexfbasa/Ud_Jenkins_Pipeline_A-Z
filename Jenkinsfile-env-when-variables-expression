pipeline {
    agent any
    environment {
        USER_EXECUTOR = "${user}"
        DEPLOY_TO = "${packageType}"
    }
    stages {
        stage('Build all paremeter') {
            when {
                allOf {
                    environment name: 'USER_EXECUTOR', value: 'Alex'
                    environment name: 'DEPLOY_TO', value: '1.0'
                }
            }
            steps {
                echo "Deploying in ${packageType}"
            }
        }
    }
}

